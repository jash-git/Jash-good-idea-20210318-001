(function($){'use strict';Drupal.behaviors.toc_js={attach:function(context,settings){var verboseIdCache={};$('.toc-js',context).each(function(){var nav=$(this).find('nav');var options={listType:$(this).data('list-type')=='ol'?'<ol/>':'<ul/>',selectors:$(this).data('selectors')||'h2, h3',container:$(this).data('container')||'body',scrollToOffset:$(this).data('scroll-to-offset')||0,prefix:$(this).data('prefix')||'toc',backToTop:!!$(this).data('back-to-top'),backToTopLabel:$(this).data('back-to-top-label')||'Back to top',letterCase:$(this).data('letter-case')||'lowercase',highlightOnScroll:!!$(this).data('highlight-on-scroll'),highlightOffset:$(this).data('highlight-offset')||100,};if(!$(this).data('smooth-scrolling')){options.smoothScrolling=false;}
