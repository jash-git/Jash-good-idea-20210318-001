function htmlEscape(str){return str.toLowerCase().replace(">","").replace("<","").replace(";","").replace("script","").replace("alert","").replace("img","").replace("frame","").replace("input","").replace("title","").replace("body","")}function getAdSrVariable(){var query=location.search.substring(1);var vars=query.split("&");for(var i=0;i<vars.length;i++){var pair=vars[i].split("=");if(pair[0]=="AD_SR"||pair[0]=="ad_sr"){return pair[1]}}return""}function getOS(userAgent){if(userAgent.match(/Windows Phone/i)){return"WindowsPhone"}else{if(userAgent.match(/Android/i)){return"Android"}else{if(userAgent.match(/iPhone|iPad|iPod/i)){return"iOS"}else{if(userAgent.match(/Windows NT/i)){return"Windows"}else{if(userAgent.match(/Macintosh/i)){return"MacOS"}else{return"Others"}}}}}}function getBrowser(userAgent){if(userAgent.match(/IEMobile/i)){return"IEMobile"}else{if(userAgent.match(/Edge/i)){return"Edge"}else{if(userAgent.match(/Chrome/i)&&userAgent.match(/Safari/i)){return"Chrome"}else{if(userAgent.match(/Firefox/i)){return"Firefox"}else{if(userAgent.match(/Safari/i)){return"Safari"}else{if(userAgent.match(/.NET/i)){return"IE"}else{if(userAgent.match(/Opera/i)){return"Opera"}else{if(userAgent.match(/Chrome/i)){return"Chrome"}else{return"Others"}}}}}}}}}function getSubDomain(pageUrl,depth){var hrefSplit=pageUrl.split("/");var result="";if(hrefSplit.length>=4){for(i=0;i<depth;i++){if(hrefSplit.length==4&&hrefSplit[3].length==0){result="Web-Index";break}else{if(hrefSplit.length>3+i){result+=(hrefSplit[3+i]+"-")}else{break}}}}else{result="Web-Index"}return result}function dmpSr_view_log(dmpSr,sp_variable,memberId){var v_check=true;var info=sp_variable.split(",")[0]+","+sp_variable.split(",")[1]+","+sp_variable.split(",")[2]+","+sp_variable.split(",")[3];var userAgent=navigator.userAgent;var isMobile={Android:function(){return userAgent.match(/Android/i)},BlackBerry:function(){return userAgent.match(/BlackBerry/i)},iOS:function(){return userAgent.match(/iPhone|iPad|iPod/i)},Opera:function(){return userAgent.match(/Opera Mini/i)},Windows:function(){return userAgent.match(/IEMobile/i)},any:function(){return(isMobile.Android()||isMobile.BlackBerry()||isMobile.iOS()||isMobile.Opera()||isMobile.Windows())}};var url=htmlEscape(location.href);var refer_url=htmlEscape(document.referrer);var platform="PC";if(isMobile.any()){platform="Mobile"}var os=getOS(userAgent);if(os=="iOS"&&edmpvct<3){edmpvct+=1;if(typeof(ElandTracker.getFingerPrint)!=="function"){v_check=false;setTimeout(function(){dmpSr_click_log(dmpSr,sp_variable,memberId)},200)}}if(v_check){var browser=getBrowser(userAgent);var ifrm=document.createElement("iframe");var subDomain=getSubDomain(url,1);var AD_SR=getAdSrVariable();var urlValue="https://cm.g.doubleclick.net/pixel?google_nid=clickforce_dmp&google_cm&log_type=view";var para="";para+="&DMP_SR="+dmpSr;para+="&info="+info;para+="&target=ad";para+="&url="+encodeURIComponent(url)+"&refer_url="+encodeURIComponent(refer_url);para+="&platform="+platform;para+="&os="+os;para+="&browser="+browser;para+="&adSrTag="+AD_SR;para+="&subDomain="+subDomain;para+="&cf_uid="+memberId;if(navigator.cookieEnabled){para+="&ce=1"}else{para+="&ce=0"}if(os=="iOS"&&typeof(ElandTracker.getFingerPrint)=="function"){para+="&fp="+ElandTracker.getFingerPrint()}ifrm.setAttribute("src",urlValue+para);ifrm.style.width=0+"px";ifrm.style.height=0+"px";ifrm.style.border=0+"px";ifrm.style.display="none";document.body.appendChild(ifrm)}}function dmpSr_click_log(dmpSr,sp_variable,memberId){var c_check=true;var info=sp_variable.split(":")[0]+","+sp_variable.split(":")[1]+","+sp_variable.split(":")[2]+","+sp_variable.split(":")[3];var userAgent=navigator.userAgent;var isMobile={Android:function(){return userAgent.match(/Android/i)},BlackBerry:function(){return userAgent.match(/BlackBerry/i)},iOS:function(){return userAgent.match(/iPhone|iPad|iPod/i)},Opera:function(){return userAgent.match(/Opera Mini/i)},Windows:function(){return userAgent.match(/IEMobile/i)},any:function(){return(isMobile.Android()||isMobile.BlackBerry()||isMobile.iOS()||isMobile.Opera()||isMobile.Windows())}};var url=htmlEscape(location.href);var refer_url=htmlEscape(document.referrer);var platform="PC";if(isMobile.any()){platform="Mobile"}var os=getOS(userAgent);if(os=="iOS"&&edmpcct<3){edmpcct+=1;if(typeof(ElandTracker.getFingerPrint)!=="function"){c_check=false;setTimeout(function(){dmpSr_click_log(dmpSr,sp_variable,memberId)},100)}}if(c_check){var browser=getBrowser(userAgent);var ifrm=document.createElement("iframe");var subDomain=getSubDomain(url,1);var AD_SR=getAdSrVariable();var urlValue="https://cm.g.doubleclick.net/pixel?google_nid=clickforce_dmp&google_cm&log_type=click";urlValue+="&DMP_SR="+dmpSr;urlValue+="&info="+info;urlValue+="&target=ad";urlValue+="&url="+encodeURIComponent(url)+"&refer_url="+encodeURIComponent(refer_url);urlValue+="&platform="+platform;urlValue+="&os="+os;urlValue+="&browser="+browser;urlValue+="&adSrTag="+AD_SR;urlValue+="&subDomain="+subDomain;urlValue+="&cf_uid="+memberId;if(navigator.cookieEnabled){urlValue+="&ce=1"}else{urlValue+="&ce=0"}if(os=="iOS"&&typeof(ElandTracker.getFingerPrint)=="function"){urlValue+="&fp="+ElandTracker.getFingerPrint()}ifrm.setAttribute("src",urlValue);ifrm.style.width=0+"px";ifrm.style.height=0+"px";ifrm.style.border=0+"px";ifrm.style.display="none";document.body.appendChild(ifrm)}};